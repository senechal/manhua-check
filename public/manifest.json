{
  "manifest_version": 3,
  "name": "Manhua-Check",
  "version": "1.0",
  "description": "A simple Manhua Chapter counter",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "page": "bg.html"
  },
  "permissions": [
    "storage"
  ]
}
